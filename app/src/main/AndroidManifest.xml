<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scitmaster.memoria">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/memoria"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/memoria"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Splash"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar"></activity>
        <activity
            android:name=".WriteSub"
            android:label="WriteSub"></activity>
        <activity
            android:name=".ListSub"
            android:label="ListSub"></activity>
        <activity
            android:name=".GameSub"
            android:label="GameSub" />
        <activity android:name=".GameSub1_2"
            android:label="GameSub1_2"></activity>
        <activity
            android:name=".GameSub2"
            android:label="GameSub2" />
        <activity
            android:name=".Videosub"
            android:label="Videosub" />
        <activity
            android:name=".EditSub"
            android:label="EditSub" />



    </application>

</manifest>